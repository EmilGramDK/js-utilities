{
  "type": "module",
  "sideEffects": false,
  "name": "@emilgramdk/web",
  "version": "0.0.1",
  "description": "Utilities for web development with TypeScript.",
  "packageManager": "bun@1.2.19",
  "keywords": [
    "web",
    "utilities",
    "typescript",
    "solid-js"
  ],
  "author": "EmilGramDK <hi@emilgram.dk>= (https://emilgram.dk)",
  "license": "ISC",
  "homepage": "https://github.com/EmilGramDK/web#readme",
  "repository": "github:EmilGramDK/web",
  "bugs": {
    "url": "https://github.com/EmilGramDK/web/issues"
  },
  "bin": {
    "setup": "./scripts/setup.ts"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.{ts,mts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,mts,tsx,js,json,md}'",
    "setup": "bun run ./scripts/setup.ts",
    "postinstall": "bun run setup"
  },
  "types": "src/index.d.ts",
  "exports": {
    "./auth": "./src/auth/index.ts",
    "./config": "./src/config/index.ts",
    "./core": "./src/core/index.ts",
    "./data": "./src/data/index.ts",
    "./date": "./src/date/index.ts",
    "./dom": "./src/dom/index.ts",
    "./hooks": "./src/hooks/index.ts",
    "./network": "./src/network/index.ts",
    "./solid": "./src/solid/index.ts",
    "./store": "./src/store/index.ts"
  },
  "files": [
    "src",
    "scripts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": ">=5.2.2",
    "eslint-plugin-import-x": ">=4.16.1",
    "eslint-plugin-perfectionist": ">=4.0.0",
    "eslint-plugin-solid": ">=0.14.5",
    "eslint-plugin-sonarjs": ">=3.0.0",
    "eslint-plugin-unicorn": ">=60.0.0",
    "eslint": ">=9.32.0",
    "globals": ">=16.3.0",
    "prettier": ">=3.6.0",
    "typescript": ">=5.8.2",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript-cookie": ">=1.0.6",
    "solid-element": ">=1.9.1",
    "solid-js": ">=1.9.7"
  }
}
